<h2>Edit <%= @volunteer.name %></h2>

<form action="/volunteers/<%=@volunteer.id %>" method="POST">
  <label> Volunteer </label>
  <input type="text" name="name" value="<%=@volunteer.name %>"> </br>

  <label> Interests </label>
  <input type="text" name="interest" value="<%=@volunteer.interest %>"> </br>

  <input type="submit" value="Update Volunteer">
</form>
